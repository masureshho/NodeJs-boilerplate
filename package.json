{
  "name": "chatmuse-admin",
  "version": "1.0.0",
  "description": "Project Description",
  "main": "index.js",
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "preinstall": "npm i -g nodemon node-inspector eslint mocha rimraf npm-run-all npm-check",
    "start": "node dist/index.js",
    "postinstall": "npm-run-all build",
    "start:simple": "nodemon src/index.js --exec babel-node --presets es2015,stage-2",
    "start:dev": "npm-run-all -s clean lint build:swagger -p watch:*",
    "start:debug": "npm-run-all -s clean build:swagger -p debug",
    "start:prod": "npm-run-all -s clean lint build serve",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "debug": "babel-node-debug",
    "watch:node": "nodemon src/index --exec babel-node --presets es2015,stage-2 --debug --delay 2.5",
    "build": "npm-run-all -s build:swagger build:babel",
    "build:swagger": "./node_modules/.bin/babel-node ./src/tasks/swagger.js --presets es2015,stage-2",
    "build:babel": "./node_modules/.bin/babel src -d dist",
    "lint": "eslint *.js src/*.js src/**/*.js",
    "test": "npm-run-all -s test:cov",
    "test:api": "NODE_ENV=test mocha --require babel-register src/**/*.api.spec.js -R spec --bail",
    "test:unit": "NODE_ENV=test mocha --require babel-register src/**/*.unit.spec.js -R spec --bail",
    "test:cov": "NODE_ENV=test babel-istanbul cover node_modules/mocha/bin/_mocha -- src/**/*unit.spec.js -R spec --bail"
  },
  "dependencies": {
    "async": "^2.2.0",
    "axios": "^0.16.2",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "config": "^1.21.0",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "express-ping": "^1.4.0",
    "express-winston": "^1.4.2",
    "glob": "^7.0.5",
    "helmet": "^3.5.0",
    "ioredis": "^2.5.0",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.6.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.9.0",
    "npm-run-all": "^3.0.0",
    "pluralize": "^4.0.0",
    "swagger-jsdoc": "^1.3.1",
    "swagger-tools": "^0.10.1",
    "validate.js": "^0.11.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^3.2.0",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.0.0",
    "sinon": "^2.1.0",
    "supertest": "^3.0.0"
  },
  "author": "Suresh Mainali",
  "license": "ISC"
}